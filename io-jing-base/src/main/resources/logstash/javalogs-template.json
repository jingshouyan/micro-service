{
  "java-logs": {
    "template": "java-logs-*",
    "settings": {
      "index.number_of_shards": 5,
      "number_of_replicas": 1
    },
    "mappings": {
      "logs": {
        "properties": {
          "@timestamp": {
            "type": "date",
            "format": "dateOptionalTime",
            "doc_values": true
          },
          "timestamp": {
            "type": "date",
            "format": "dateOptionalTime",
            "doc_values": true
          },
          "logdate": {
            "type": "keyword"
          },
          "server": {
            "type": "keyword"
          },
          "instance": {
            "type": "keyword"
          },
          "traceId": {
            "type": "keyword"
          },
          "level": {
            "type": "keyword"
          },
          "thread": {
            "type": "keyword"
          },
          "logger": {
            "type": "keyword"
          },
          "msg": {
            "type": "text"
          }
        }
      }
    }
  }
}
